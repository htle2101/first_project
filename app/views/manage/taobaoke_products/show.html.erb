<%- model_class = @taobaoke_product.class -%>
<h1><%=t '.title', :default => model_class.model_name.human %></h1>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:cid) %>:</strong></dt>
  <dd><%= @taobaoke_product.cid %></dd>
  <dt><strong><%= model_class.human_attribute_name(:taobao_id) %>:</strong></dt>
  <dd><%= @taobaoke_product.taobao_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @taobaoke_product.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nick) %>:</strong></dt>
  <dd><%= @taobaoke_product.nick %></dd>
  <dt><strong><%= model_class.human_attribute_name(:price) %>:</strong></dt>
  <dd><%= @taobaoke_product.price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pic_url) %>:</strong></dt>
  <dd><%= image_tag("#{@taobaoke_product.pic_url}_250x250.jpg") %></dd>
  <dt><strong><%= model_class.human_attribute_name(:click_url) %>:</strong></dt>
  <dd><%= @taobaoke_product.click_url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:commission) %>:</strong></dt>
  <dd><%= @taobaoke_product.commission %></dd>
  <dt><strong><%= model_class.human_attribute_name(:commission_rate) %>:</strong></dt>
  <dd><%= @taobaoke_product.commission_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:commission_num) %>:</strong></dt>
  <dd><%= @taobaoke_product.commission_num %></dd>
  <dt><strong><%= model_class.human_attribute_name(:commission_volume) %>:</strong></dt>
  <dd><%= @taobaoke_product.commission_volume %></dd>
  <dt><strong><%= model_class.human_attribute_name(:coupon_price) %>:</strong></dt>
  <dd><%= @taobaoke_product.coupon_price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:coupon_rate) %>:</strong></dt>
  <dd><%= @taobaoke_product.coupon_rate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:coupon_start_time) %>:</strong></dt>
  <dd><%= @taobaoke_product.coupon_start_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:coupon_end_time) %>:</strong></dt>
  <dd><%= @taobaoke_product.coupon_end_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:volume) %>:</strong></dt>
  <dd><%= @taobaoke_product.volume %></dd>
  <dt><strong><%= model_class.human_attribute_name(:shop_click_url) %>:</strong></dt>
  <dd><%= @taobaoke_product.shop_click_url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:seller_credit_score) %>:</strong></dt>
  <dd><%= @taobaoke_product.seller_credit_score %></dd>
  <dt><strong><%= model_class.human_attribute_name(:item_location) %>:</strong></dt>
  <dd><%= @taobaoke_product.item_location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:shop_type) %>:</strong></dt>
  <dd><%= @taobaoke_product.shop_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:taobaoke_cat_click_url) %>:</strong></dt>
  <dd><%= @taobaoke_product.taobaoke_cat_click_url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:keyword_click_url) %>:</strong></dt>
  <dd><%= @taobaoke_product.keyword_click_url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @taobaoke_product.status %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
    manage_taobaoke_products_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
    edit_manage_taobaoke_product_path(@taobaoke_product), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
    manage_taobaoke_product_path(@taobaoke_product),
    :method => 'delete',
    :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
    :class => 'btn btn-danger' %>
</div>
